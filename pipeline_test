def MyName = "Rajesh"

pipeline {
agent any 
stages {
stage ("print variable") {
steps { 
echo "My Name is ${MyName}"
    }
   }
  }
  stage ("build") {
  steps {
  git url: 'https://github.com/rajeshgitnew/jenkins1.git', branch: 'Main'
  sh "chmod +R ./helloworld.sh"
  sh "./helloworld.sh"
   }
  }
 }
