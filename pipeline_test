def MyName = "Rajesh"

pipeline {
agent any 
stages {
stage ("print variable") {
steps { 
echo "My Name is ${MyName}"
    }
   }
stage ("build") {
steps {
git url: 'https://github.com/rajeshgitnew/jenkins1', branch: 'Main'
sh "chmod +x -R ./jenkins1/helloworld.sh"
sh "./jenkins1/helloworld.sh"
   }
  }
 }
}
